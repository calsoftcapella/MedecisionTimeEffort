	}
	function onEntry(url) {
		var submitForm = document.forms[0];
		submitForm.action = url;
		submitForm.submit();
	}
	function onEdit(url) {
		var submitForm = document.forms[0];
		submitForm.action = url;
		submitForm.submit();
	}
