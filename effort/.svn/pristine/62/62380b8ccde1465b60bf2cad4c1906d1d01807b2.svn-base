											</logic:notEqual></td>
										<td><logic:equal value="null" name="taskFormList1"
												property="task_id"></logic:equal> <logic:notEqual
												value="null" name="taskFormList1" property="task_id">
												<bean:write name="taskFormList1" property="task_id" />
